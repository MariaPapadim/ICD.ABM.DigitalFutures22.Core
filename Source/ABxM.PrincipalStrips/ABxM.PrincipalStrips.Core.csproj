    <TargetExt>.dll</TargetExt>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Grasshopper" Version="7.13.21348.13001" IncludeAssets="compile;build" />
  </ItemGroup>
  
  <ItemGroup>
    <Folder Include="Environments\" />
  </ItemGroup>
  
  <ItemGroup>
    <Reference Include="ABxM.Core">
